{
    "name": "kiroushi/laravel-db-blade",
    "description": "Render Blade templates from Eloquent Model Fields",
    "keywords": [
        "laravel",
        "blade",
        "compiler",
        "eloquent",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier PÃ©rez <Kiroushi>",
            "email": "kiroushi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kiroushi\\DbBlade\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Kiroushi\\DbBlade\\DbBladeCompilerServiceProvider"
            ],
            "aliases": {
                "DbView": "Kiroushi\\DbBlade\\Facades\\DbView"
            }
        }
    }
}
